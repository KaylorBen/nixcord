{
  "name": "generate-plugin-options",
  "version": "1.0.0",
  "description": "Extract Vencord plugin settings and generate Nix options",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "generate-plugin-options": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --loader ts-node/esm ./src/index.ts",
    "dev": "tsx watch ./src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\""
  },
  "author": "FlameFlag",
  "license": "MIT",
  "dependencies": {
    "change-case": "^5.4.4",
    "commander": "^14.0.2",
    "consola": "^3.4.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "iter-tools": "^7.5.4",
    "ora": "^9.0.0",
    "p-limit": "^7.2.0",
    "pathe": "^2.0.3",
    "picocolors": "^1.1.1",
    "remeda": "^2.32.0",
    "true-myth": "^9.2.0",
    "ts-morph": "^27.0.2",
    "ts-pattern": "^5.9.0",
    "type-fest": "^5.2.0",
    "zod": "^4.1.12",
    "zod-validation-error": "^5.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "fast-check": "^4.3.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9"
  }
}
